   "source": [
    "d01 = {\n",
    "    'name': 'John Doe',\n",
    "    'age': 18,\n",
    "    'hobby': 'Learning',\n",
    "}\n",
    "\n",
    "for key in d01.keys():\n",
    "    print(key, end=',   ')\n",
    "print()\n",
    "\n",
    "for val in d01.values():\n",
    "    print(val, end=',   ')\n",
    "print()\n",
    "\n",
    "for key, val in d01.items():\n",
    "    print(f'{key}={val}', end=',   ')\n",
    "print()\n",
    "\n"
   ]
  }
